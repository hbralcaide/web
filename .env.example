# Public environment variables (safe to expose to browser - prefixed with VITE_)
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_SUPABASE_FUNCTIONS_URL=your_supabase_functions_url

# MappedIn public credentials
VITE_MAPPEDIN_SKIP_MAPDATA=true
VITE_MAPPEDIN_API_KEY=your_mappedin_api_key
VITE_MAPPEDIN_MAP_ID=your_mappedin_map_id
VITE_MAPPEDIN_MVF_URL=/mappedin/market.mvf.zip

# SERVER-ONLY environment variables (NOT exposed to browser - NO VITE_ prefix)
# These are used only in Supabase Edge Functions or backend processes
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
MAPPEDIN_SECRET=your_mappedin_secret
